56
64
#Using Range function  print multiples of 5 from 75 to 0
for i in range(75,0,-5):
    if i%5==0:
        print(i)
##output## 
75
70
65
60
55
50
45
40
35
30
25
20
15
10

#Using Range function  print multiples of 8 from 96 to 72
for i in range(96,72,-8):
    if i%8==0:
        print(i)
##output## 
96
88
80

Task2:
#Get a dynamic list from user
list1=[]
number=int(input("enter list length:"))
print("enter the number:")
for i in range(number):
    data=int(input())
    list1.append(data)
print(list1)
##output##
enter list length:5
enter the number:
21
22
23
24
25
[21, 22, 23, 24, 25]

#Task3:
#Get a dynamic dictionary from user
#clues Task2 and Task3
#1. get number of elements from user
#Loop through range
#append to list/dicti

dict={}
element=int(input("enter the element:"))
for i in range(element):
    key=input("enter the key:")
    value=input("enter the value:")
    dict[key]=value
print(dict)
##output##
enter the element:3
enter the key:1
enter the value:aaa
enter the key:2
enter the value:ggg
enter the key:3
enter the value:rrr
{'1': 'aaa', '2': 'ggg', '3': 'rrr'}

#Task4:
#Get two integers from user
#print multiples of 8 between them

#clues:
#Use range function in for loop
#Use if condition inside for loop
#add in to list

#input 10 100
#multiples of 8
#[16,24,32....,96]

a=int(input("enter the starting range:"))
b=int(input("enter the ending range:"))
list=[]
for i in range(a,b):
    if i%8==0:
        list.append(i)
print(list)
##output##
enter the starting range:10
enter the ending range:100
[16, 24, 32, 40, 48, 56, 64, 72, 80, 88, 96]

#Task5:
#Input:
#Li1 = [3,4,5,2,7,8,9,10]

#Output:
#Li_odd = [3,5,7,9]
#Li_even = [4,2,8,10]
#Li1 = [3,4,5,2,7,8,9,10]

List1 = [3,4,5,2,7,8,9,10]
odd=[]
even=[]
for i in List1:
    if i%2==0:
        even.append(i)
    else:
        odd.append(i)
print("odd numbers of the list1:",odd)
print("even numbers of the list1:",even)
##output##
odd numbers of the list1: [3, 5, 7, 9]
even numbers of the list1: [4, 2, 8, 10]

#Task6:
#input: [-1, -7,8,10,20,21,17,28,-3,0,0,]
    
#Output:
#neg_LI = [-1,-7,-3]
#pos_LI = []
#Zeros = []

#Numeber of postivie ele: 7
#Number nega: 3
#Number of zeros: 2

list1=[-1, -7,8,10,20,21,17,28,-3,0,0,]
negative=[]
positive=[]
zero=[]
for i in list1:
    if i>0:
        positive.append(i)
    elif i<0:
        negative.append(i)
    elif i==0:
        zero.append(i)
print("positive elements:",positive)
print("negative elements:",negative)
print("zero:")
print("length of the positive elements:",len(positive))
print("length of the negative elements:",len(negative))
print("length of the zero elements:",len(zero))
##output##
sitive elements: [8, 10, 20, 21, 17, 28]
negative elements: [-1, -7, -3]
zero:
length of the positive elements: 6
length of the negative elements: 3
length of the zero elements: 2















